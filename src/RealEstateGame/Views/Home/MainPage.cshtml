

<div class="containter">
    <div class="col-md-6">
        @{
            var player = ViewBag.Player;
        }
        @Html.DisplayFor(m => player, "view-player")

        @foreach (var home in Model.GetOwnedHomes())
        {
            @Html.DisplayFor(m => home, "list-home")
        }
    </div>
    <div class="col-md-6" id="rightside">
        @Html.Partial(ViewData["partial"].ToString())
    </div>
</div>