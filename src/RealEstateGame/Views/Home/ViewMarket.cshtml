@model RealEstateGame.Models.Player

<div class="container">
    <div class="col-md-3">
        @Html.DisplayFor(m => m, "view-player")
    </div>
    <div class="col-md-9">
        @foreach (var home in ViewBag.Homes)
        {
            @Html.DisplayFor(model => home, "list-home")
        }
    </div>
</div>